<h1>投稿一覧</h1>

<h1 class="jquery">Hello jQuery!!</h1>

<div id="container">
	<div id="theTarget">
		<div class="img1"></div>
		<div class="img2"></div>
		<div class="img3"></div>
		<div class="img4"></div>
		<div class="img5"></div>
	</div>
</div>

  <% @lists.each do |list| %>
    <p>タイトル</p>
    <span>
    	<%= link_to list.title, todolist_path(list.id) %>
    </span>
    <p>画像</p>
    <%= attachment_image_tag list, :image, :fill, 100, 100, format: 'jpeg' %>
  <% end %>